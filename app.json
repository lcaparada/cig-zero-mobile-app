{
  "expo": {
    "name": "CigZero",
    "slug": "cig-zero-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#7EA2AA"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "3",
      "bundleIdentifier": "com.elevanteholdings.cig-zero"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7EA2AA"
      },
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "package": "com.elevanteholdings.cigzero"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SF-Pro-Rounded-Regular.otf",
            "./assets/fonts/SF-Pro-Rounded-Medium.otf",
            "./assets/fonts/SF-Pro-Rounded-Semibold.otf",
            "./assets/fonts/SF-Pro-Rounded-Bold.otf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "elevate-holdings-70",
          "project": "4508123314651136",
          "url": "https://sentry.io/"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bd47c4d6-98ce-4ff1-beaf-21891fddbfd4"
      }
    },
    "owner": "cavalcante.lucasx"
  }
}
